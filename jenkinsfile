node {
    stage('SCM') {
	echo 'Gathering code from version control'
    }
    stage('Build') {
	try{
        echo 'Building....'
    	sh 'dotnet --version'
    	sh "dotnet build ConsoleApp1"
        echo 'Building New Feature'
	    releasenotes();
        }catch(ex){
        	echo 'Something went wrong'
        	echo ex.toString();
        	currentBuild.result = 'FAILURE'
        }
        finally{
        	// cleanup
        }
    }
    stage('Test') {
        echo 'Testing....'
    }
    stage('Deploy') {
        echo 'Deploying....'
    }
}